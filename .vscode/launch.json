{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceRoot}\\src\\dotnet-frc\\bin\\Debug\\netcoreapp1.0\\dotnet-frc.dll",
            "args": [
                "C:\\Users\\thadh\\Documents\\VSTests\\src\\Robot451",
                "deploy",
                "-t", 
                "9999"
            ],
            "cwd": "${workspaceRoot}",
            "externalConsole": false,
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command.pickProcess}"
        }
    ]
}